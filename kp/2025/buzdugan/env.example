# Настройки приложения
PROJECT_NAME=Privacy-Preserving Database Proxy
VERSION=1.0.0
ENVIRONMENT=development
SECRET_KEY=your-secret-key-here-change-in-production

# База данных
DATABASE_URL=postgresql://admin:secure_password_123@localhost:5432/privacy_proxy

# Redis
REDIS_URL=redis://localhost:6379

# CORS
ALLOWED_HOSTS=["*"]

# Безопасность
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Приватность
DEFAULT_K_ANONYMITY=5
DEFAULT_L_DIVERSITY=3
DEFAULT_EPSILON=1.0
DEFAULT_DELTA=0.00001

# Логирование
LOG_LEVEL=INFO
LOG_FORMAT=json

# Мониторинг
ENABLE_METRICS=true
METRICS_PORT=9090
